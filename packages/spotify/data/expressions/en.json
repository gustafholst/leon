{
  "spotify": {
     "play_track": {
      "expressions": [
        "Play",
        "Resume",
        "Play maiden voayage",
        "Play smells like teen spirit",
        "Play the song stairway to heaven",
        "Play the track lucy in the sky with diamonds by the beatles",
        "Play me the song roxanne by the police",
        "Play the tune fly me to the moon by Frank Sinatra"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "track",
          "conditions": [
            {
              "type": "after",
              "from": "play"
            },
            {
              "type": "after",
              "from": "song"
            },
            {
              "type": "after",
              "from": "track"
            },
            {
              "type": "after",
              "from": "tune"
            },
            {
              "type": "between",
              "from": "song",
              "to": "by"
            },
            {
              "type": "between",
              "from": "track",
              "to": "by"
            },
            {
              "type": "between",
              "from": "tune",
              "to": "by"
            }
          ]
        },
        {
          "type": "trim",
          "name": "artist",
          "conditions": [
            {
              "type": "after",
              "from": "artist"
            },
            {
              "type": "after",
              "from": "group"
            },
            {
              "type": "after",
              "from": "by"
            }
          ]
        }
      ]
    },
    "play_album": {
      "expressions": [
        "Play album foo",
        "Play the album derp by foo fighters"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "album",
          "conditions": [
            {
              "type": "between",
              "from": "album",
              "to": "by"
            },
            {
              "type": "after",
              "from": "album"
            }
          ]
        },
         {
          "type": "trim",
          "name": "artist",
          "conditions": [
            {
              "type": "after",
              "from": "artist"
            },
            {
              "type": "after",
              "from": "group"
            },
            {
              "type": "after",
              "from": "by"
            }
          ]
        }
      ]
    },
     "play_playlist": {
      "expressions": [
        "Play playlist foo",
        "Play the playlist derp"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "playlist",
          "conditions": [
            {
              "type": "after",
              "from": "playlist"
            }
          ]
        }
      ]
    },
    "login" : {
      "expressions": [
        "spotify login",
        "login to spotify"
      ]
    }
  },
  "authorize" : {
    "run" : {
      "expressions": [
        "http://localhost:1337/callback?code= "
      ],
      "entities": [
        {
          "type": "regex",
          "name": "auth_url",
          "regex": "^http://localhost:1337/callback?code="
        }
      ]
    }
  }
}
